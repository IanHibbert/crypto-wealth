    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-runtime": "6.20.0",
    "body-parser": "1.15.2",
    "bootstrap-loader": "2.0.0-beta.16",
    "bootstrap-sass": "3.3.7",
    "classnames": "2.2.5",
    "clean-webpack-plugin": "0.1.14",
