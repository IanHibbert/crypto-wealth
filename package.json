{
  "name": "cryptowealth",
  "version": "0.0.1",
  "description": "A cryptocurrency investment platform.",
  "scripts": {
    "global-install": "npm install -g eslint sass-lint webpack",
    "heroku-postbuild": "npm run prod && python src/manage.py collectstatic --noinput --settings=crypto.settings.prod",
    "dev": "webpack --progress --display-error-details --config webpack/common.config.js --watch",
    "prod": "webpack --progress -p --config webpack/common.config.js",
    "validate": "npm run lint && npm run analyze",
    "lint": "npm run lintjs && npm run lintscss",
    "analyze": "npm run prospector && npm run bandit",
    "prospector": "prospector  --profile-path=. --profile=.prospector.yml --path=src --ignore-patterns=static || true",
    "bandit": "bandit -r src || true",
    "lintjs": "eslint --fix -c .eslintrc src/static tests/js || true",
    "lintscss": "sass-lint --verbose || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csc301-winter-2017/project-team-10.git"
  },
  "engines": {
    "node": "^7.0.0",
    "npm": "^4.0.0"
  },
  "author": "Aaron, Akshay, Harold, Ian, Sakshaat",
  "license": "GPL-3.0",
